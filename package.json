{
  "name": "@distributed/utm",
  "description": "Small library to parse utm parameters",
  "version": "0.0.6",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "module": "dist/index.mjs",
  "repository": "https://github.com/distributedvc/utm",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "utm",
    "parameters",
    "params",
    "library"
  ],
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=10"
  },
  "prettier": {
    "printWidth": 100,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "resolutions": {
    "eslint-config-prettier": "8.3.0"
  },
  "scripts": {
    "start": "tsup src/index.ts --format esm,cjs --watch --dts",
    "build": "rimraf dist/ && tsup src/index.ts --format esm,cjs --dts --minify",
    "test": "yarn lint && yarn vitest run --dom",
    "lint": "eslint --ext .ts src/",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "release": "yarn test && standard-version && git push --follow-tags && npm publish"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "yarn lint"
    }
  },
  "size-limit": [
    {
      "path": "dist/index.js",
      "limit": "3 KB"
    },
    {
      "path": "dist/index.mjs",
      "limit": "3 KB"
    }
  ],
  "devDependencies": {
    "@commitlint/cli": "17.0.3",
    "@commitlint/config-conventional": "17.0.3",
    "@size-limit/preset-small-lib": "8.0.1",
    "@swc/core": "1.2.242",
    "@tsconfig/recommended": "1.0.1",
    "@types/node": "18.7.13",
    "@typescript-eslint/eslint-plugin": "5.35.1",
    "@typescript-eslint/parser": "5.35.1",
    "@vercel/node": "2.5.11",
    "eslint": "8.22.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.2.1",
    "happy-dom": "^6.0.4",
    "husky": "8.0.1",
    "prettier": "2.7.1",
    "rimraf": "3.0.2",
    "size-limit": "8.0.1",
    "standard-version": "9.5.0",
    "tsup": "6.2.2",
    "typescript": "4.8.2",
    "vitest": "0.22.1"
  },
  "dependencies": {
    "query-string": "7.1.1"
  },
  "packageManager": "yarn@3.2.3"
}
