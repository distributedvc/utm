{
  "name": "@distributed/utm",
  "type": "module",
  "description": "Small library to parse utm parameters",
  "version": "0.1.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "module": "dist/index.mjs",
  "repository": "https://github.com/distributedvc/utm",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "utm",
    "parameters",
    "params",
    "library"
  ],
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=10"
  },
  "prettier": {
    "printWidth": 100,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "scripts": {
    "start": "tsup src/index.ts --format esm,cjs --watch --dts",
    "build": "rimraf dist/ && tsup src/index.ts --format esm,cjs --dts --minify",
    "test": "yarn lint && yarn vitest run --dom",
    "lint": "eslint --ext .ts src/",
    "prepare": "yarn build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "release": "yarn test && standard-version && git push --follow-tags && npm publish"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "yarn lint"
    }
  },
  "size-limit": [
    {
      "path": "dist/index.js",
      "limit": "3 KB"
    },
    {
      "path": "dist/index.mjs",
      "limit": "3 KB"
    }
  ],
  "devDependencies": {
    "@commitlint/cli": "17.7.2",
    "@commitlint/config-conventional": "17.7.0",
    "@size-limit/preset-small-lib": "9.0.0",
    "@swc/core": "1.3.92",
    "@tsconfig/recommended": "1.0.3",
    "@types/node": "18.18.4",
    "@typescript-eslint/eslint-plugin": "6.7.5",
    "@typescript-eslint/parser": "6.7.5",
    "@vercel/node": "3.0.7",
    "eslint": "8.51.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-prettier": "5.0.1",
    "happy-dom": "^12.9.1",
    "husky": "8.0.3",
    "prettier": "3.0.3",
    "rimraf": "5.0.5",
    "size-limit": "9.0.0",
    "standard-version": "9.5.0",
    "tsup": "7.2.0",
    "typescript": "5.2.2",
    "vitest": "0.34.6"
  },
  "dependencies": {
    "query-string": "8.1.0"
  },
  "packageManager": "yarn@4.0.2"
}
